package SelectionSort;

public class SelectionSort {

	
	
	for(static int i=0;i<=arr.length;i++) {
		
		int[] arr;
		for(int j=1;j<=arr.length;j++)
		{

			if(arr[i]>arr[j+1])
			{
				int temp=arr[i];
				arr[i]=arr[j];
				arr[j]=temp;
				//System.out.println(temp);
						
			}
			System.out.println(arr[i]);
			
		}
	}

	public static void printme(int[] arr) {
		System.out.println(arr[i]);
		
	}
	
	
	
}
}